@model WebApplication4.Models.TbAcceso

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles</h1>

<div>
    <h4>Registro n° @Html.DisplayFor(model => model.IdAcceso)</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Correo de Acceso
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Correo)
        </dd>
        <dt class = "col-sm-2">
            Rol
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdPermisoNavigation.Nombre)
        </dd>
        <dt class = "col-sm-2">
            Usuario Asignado
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdUsuarioNavigation.Nombre)
            @Html.DisplayFor(model => model.IdUsuarioNavigation.Apellido1)
            @Html.DisplayFor(model => model.IdUsuarioNavigation.Apellido2)
        </dd>
    </dl>
</div>
<div>
    <h5 style="padding-bottom:10px;">Opciones</h5>

    <hr />
    <dd>

            <a asp-action="Edit" asp-route-id="@Model?.IdAcceso" class="btn btn-warning">Editar</a>
            <a asp-action="Index" class="btn btn-secondary">Volver</a>



    </dd>
</div>
