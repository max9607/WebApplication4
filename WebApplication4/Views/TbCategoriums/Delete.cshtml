@model WebApplication4.Models.TbCategorium

@{
    ViewData["Title"] = "Delete";
}

<h1>Eliminar</h1>

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>Registro n° @Html.DisplayFor(model => model.IdProblema)</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Problema)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Problema)
        </dd>
    </dl>
    

    <div>
        <h5 style="padding-bottom:10px;">Opciones</h5>

        <hr />
        <form asp-action="Delete">
            <input type="hidden" asp-for="IdProblema" />
            <input type="submit" value="Delete" class="btn btn-danger" />
            <a asp-action="Index" class="btn btn-secondary">Volver</a>
        </form>
    </div>
</div>
