@model IEnumerable<WebApplication4.Models.TbTicket>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>
<h1 class="display-4">Bienvenido! @User.Identity.Name</h1>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DespricionP)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DescripionDetallada)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Localizacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Adjunto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdEstadoNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdFechaNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdPrioridadNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdProblemaNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdUsuarioNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DespricionP)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DescripionDetallada)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Localizacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Adjunto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdEstadoNavigation.IdEstado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdFechaNavigation.IdFecha)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdPrioridadNavigation.IdPrioridad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdProblemaNavigation.IdProblema)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdUsuarioNavigation.IdUsuario)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdTicket">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdTicket">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdTicket">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
